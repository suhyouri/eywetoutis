      if (typeof FaceMesh !== "undefined" && typeof Camera !== "undefined") {
        initFaceMesh();
      } else {
        console.error("MediaPipe ë¼ì´ë¸ŒëŸ¬ë¦¬ë¥¼ ë¡œë“œí•  ìˆ˜ ì—†ìŠµë‹ˆë‹¤.");
        loadingOverlay.textContent = "âŒ ì›¹ìº  ì´ˆê¸°í™” ì‹¤íŒ¨";
